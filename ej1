def tabla_multiplicar():
    for i in range(1, 6):  # Iteramos del 1 al 5
        for j in range(1, 11):  # Multiplicamos cada número del 1 al 10
            producto = i * j
            if producto > 20:  # Si el producto supera 20, rompemos el bucle interno
                break
            if i % 2 == 0:  # Si el número es par, omitimos la multiplicación
                continue
            
            pass  
            print(i, "x", j, "=", producto)

tabla_multiplicar()

